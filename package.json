	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^30.0.5",
		"jest-environment-jsdom": "^30.0.5",
		"obsidian": "latest",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.1",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
	}
}
